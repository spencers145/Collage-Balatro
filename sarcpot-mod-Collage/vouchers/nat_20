SMODS.Voucher({
    key = "nat_20",
    atlas = "vouchers",
    config = {
        extra = {}
    },
    pos = {
        x = 1,
        y = 0
    },
    cost = 10,
    unlocked = true,
    discovered = false,
    available = true,
    requires = {'v_sarc_advantage'},

    calculate = function(self, card, context)
        if context.mod_probability then
            return {
                numerator = context.numerator * 2
            }
        end
    end
})
