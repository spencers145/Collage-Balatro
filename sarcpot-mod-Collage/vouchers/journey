if SARC.config.consumables_enabled then
    SMODS.Voucher {
        key = 'journey',
        atlas = 'vouchers',
        pos = {
            x = 3,
            y = 0
        },
        loc_vars = function(self, info_queue)
            return {
                vars = {
                    colours = {G.SP.C.travel_2}
                }
            }
        end,
        unlocked = true,
        discovered = false,
        available = true,
        requires = {'v_sarc_compass'},
        redeem = function(self, card)

            G.GAME.travel_rate = 3
        end
    }
end

