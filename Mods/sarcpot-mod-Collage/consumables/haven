SMODS.Consumable({
    key = 'haven',
    set = 'travel',
    atlas = 'consumables',
    pos = {x=3, y=1},
    discovered = false,
    config = {mod_conv = "m_sarc_flow", max_highlighted = 3},
    effect = 'Enhance',
    cost = 3,
    loc_vars = function(self, info_queue)
		info_queue[#info_queue + 1] = G.P_CENTERS[self.config.mod_conv]

		return { vars = { self.config.max_highlighted,self.config.mod_conv } }
	end,

})